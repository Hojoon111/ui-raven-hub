                Window.DepthOfField = Library:MakeBlurred(Items["MainFrame"], Window )

                Items["MainFrame"]:MakeDraggable()
                Items["MainFrame"]:MakeResizeable(Vector2New(697, 550), Vector2New(9999, 9999))
                
