            function Notification:Add(Name, Callback, Color, TextColor)
                local NewButton = { }
                
                local NewItems = { } do 
                    Items["Button"..Name] = Instances:Create("TextButton", {
                        Parent = Items["Buttons"].Instance,
                        Name = "\0",
                        FontFace = Library.Font,
                        TextColor3 = FromRGB(0, 0, 0),
                        BorderColor3 = FromRGB(0, 0, 0),
                        Text = "",
                        AutoButtonColor = false,
                        Size = UDim2New(1, 0, 1, 0),
                        BorderSizePixel = 0,
                        TextSize = 14,
                        BackgroundColor3 = Color
                    })
                    
                    Instances:Create("UICorner", {
                        Parent = Items["Button"..Name].Instance,
                        Name = "\0"
                    })
                    
                    Items["Text"..Name] = Instances:Create("TextLabel", {
                        Parent = Items["Button"..Name].Instance,
                        Name = Name,
                        FontFace = Library.Font,
                        TextColor3 = TextColor,
                        BorderColor3 = FromRGB(0, 0, 0),
                        Text = Name,
                        AnchorPoint = Vector2New(0.5, 0.5),
                        Size = UDim2New(0, 0, 0, 15),
                        BackgroundTransparency = 1,
                        Position = UDim2New(0.5, 0, 0.5, 0),
                        BorderSizePixel = 0,
                        AutomaticSize = Enum.AutomaticSize.X,
                        TextSize = 14,
                        BackgroundColor3 = FromRGB(255, 255, 255)
                    })                    
                end  

                function NewButton:Press()
                    Library:SafeCall(Callback)
                    Library:Thread(function()
                        for Index, Value in Items do 
                            if Value.Instance:IsA("TextLabel") then 
                                Value:Tween(nil, {TextTransparency = 1})
                            elseif Value.Instance:IsA("ImageLabel") then 
                                Value:Tween(nil, {ImageTransparency = 1})
                            elseif Value.Instance:IsA("TextButton") then 
                                Value:Tween(nil, {BackgroundTransparency = 1})
                            elseif Value.Instance:IsA("Frame") or Value.Instance:IsA("TextButton") then
                                Value:Tween(nil, {BackgroundTransparency = 1})
                            end
                        end
    
                        Items["Notification"]:Tween(nil, {Size = UDim2New(0, 0, 0, 0)})
                        task.wait(0.5)
                        Items["Notification"]:Clean()
                    end)
                end
    
                Items["Button"..Name]:Connect("MouseButton1Down", function()
                    NewButton:Press()
                end)

                return Notification
            end

            local Size = Items["Notification"].Instance.AbsoluteSize

            for Index, Value in Items do 
                if Value.Instance:IsA("Frame") and Index ~= "Notification" then
                    Value.Instance.BackgroundTransparency = 1
                elseif Value.Instance:IsA("TextLabel") then 
                    Value.Instance.TextTransparency = 1
                elseif Value.Instance:IsA("ImageLabel") then 
                    Value.Instance.ImageTransparency = 1
                elseif Value.Instance:IsA("TextButton") then 
                    Value.Instance.BackgroundTransparency = 1
                    Value.Instance.TextTransparency = 1
                end
            end 
            
            task.wait(0.2)

            Items["Notification"].Instance.AutomaticSize = Enum.AutomaticSize.Y

            Library:Thread(function()
                for Index, Value in Items do 
                    if Value.Instance:IsA("TextLabel") and Index == "Text" then 
                        Value:Tween(nil, {TextTransparency = 0})
                    elseif Value.Instance:IsA("TextLabel") and Index == "Description" then 
                        Value:Tween(nil, {TextTransparency = 0.7})
                    elseif Value.Instance:IsA("ImageLabel") then 
                        Value:Tween(nil, {ImageTransparency = 0.3})
                    elseif Value.Instance:IsA("TextButton") then 
                        Value:Tween(nil, {BackgroundTransparency = 0, TextTransparency = 0})
                    end
                end

                Items["Notification"]:Tween(nil, {Size = UDim2New(0, Size.X, 0, Size.Y)})
            end)

            return Notification
        end

        Library.Window = function(self, Data)
            Data = Data or { }

            local Window = {
                Name = Data.Name or Data.name or "Window",
                Logo = Data.Logo or Data.logo or "125018723085219",
                
                Pages = { },
                Items = { },
                IsOpen = false,
                DepthOfField = nil,
                BlockMesh = nil,
            }

            local Items = { } do
                Items["MainFrame"] = Instances:Create("Frame", {
